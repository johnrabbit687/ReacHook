{
  "name": "react-hooks.org",
  "private": true,
  "description": "Useful React hooks for everyone.",
  "scripts": {
    "clean": "lerna clean",
    "lint": "lerna run lint --stream --parallel",
    "test": "lerna run test --stream --parallel",
    "coverage": "lerna run test --stream --parallel -- --coverage",
    "dev": "cd packages/website && yarn dev",
    "deploy": "cd packages/website && yarn deploy",
    "create": "node helpers/create",
    "bs": "lerna bootstrap",
    "prebuild": "lerna clean -y && yarn",
    "build": "lerna run build --stream --parallel",
    "update-deps": "cd packages/website && yarn run update-deps",
    "prerelease": "yarn build",
    "release": "lerna publish",
    "release-individual": "lerna publish --ignore-changes=rooks"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.5",
    "@babel/plugin-external-helpers": "7.2.0",
    "@babel/plugin-proposal-export-default-from": "7.2.0",
    "@babel/preset-env": "^7.1.5",
    "@babel/preset-react": "^7.0.0",
    "@types/react": "16.7.18",
    "@types/react-dom": "16.0.11",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "8.0.5",
    "babel-plugin-prismjs": "1.0.2",
    "babel-preset-minify": "0.5.0",
    "browser-env": "3.2.5",
    "chalk": "2.4.1",
    "inquirer": "6.2.0",
    "jest": "^23.6.0",
    "lerna": "^3.4.3",
    "lodash.capitalize": "4.2.1",
    "make-dir": "1.3.0",
    "meow": "5.0.0",
    "mini-css-extract-plugin": "0.5.0",
    "ora": "3.0.0",
    "puppeteer": "1.11.0",
    "raf": "3.4.1",
    "react-testing-library": "^5.2.3",
    "read-pkg-up": "4.0.0",
    "replace-string": "2.0.0",
    "rollup": "1.1.2",
    "rollup-plugin-babel": "4.3.2",
    "rollup-plugin-commonjs": "9.2.0",
    "rollup-plugin-flow": "1.1.1",
    "rollup-plugin-json": "3.1.0",
    "rollup-plugin-node-resolve": "4.0.0",
    "rollup-plugin-replace": "2.1.0",
    "rollup-plugin-sourcemaps": "0.4.2",
    "rollup-plugin-terser": "4.0.4",
    "shelljs": "0.8.3",
    "webpack": "4.29.1",
    "webpack-cli": "3.2.1",
    "write-pkg": "3.2.0"
  },
  "dependencies": {
    "jest-localstorage-mock": "2.4.0"
  },
  "workspaces": [
    "packages/*"
  ],
  "resolutions": {
    "terser": "3.14"
  }
}
